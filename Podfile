# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Chan' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!


    # Strucutre
    pod 'RIBs'
    pod 'IGListKit', '~> 3.0'

    # RX
    pod 'RxSwift'
    pod 'RxCocoa'

    # Networking
    pod 'Alamofire'
    pod 'AlamofireImage'

    pod 'Moya'
    pod 'Moya/RxSwift'
    
    # UI
    pod 'SnapKit'
    
    # Data
    pod 'SwiftSoup'



end
